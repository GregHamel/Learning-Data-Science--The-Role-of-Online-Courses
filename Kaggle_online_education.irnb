        "_kg_hide-input": true
      },
      "cell_type": "code",
      "source": "# Prepare dataframe of platform usage by gender\nplot_frame <- multiple_choice_Q36 %>% \n    mutate(Gender = multiple_choice[2:nrow(multiple_choice),]$Q1) %>% \n    filter(Gender == \"Male\" | Gender == \"Female\") %>% \n    gather(key = \"Platform\", value = \"Num_Users\", -\"Gender\") %>%\n    group_by(Gender, Platform) %>% \n    summarise(Counts = sum(Num_Users), \n              Proportion = round(Counts/n(),2),\n              Prop_str = percent_to_string(Proportion))\n\ncustom_cols_p17 <- c('Coursera', 'Udemy', 'DataCamp', 'Udacity', \n                     'edX', 'Kaggle.Learn', 'University', 'Fast.AI')\n\n# Generate and display plot\np17 <- plot_frame %>%    \n     ggplot(aes(x = Platform, y = Proportion)) + \n     geom_bar(stat = \"identity\", color = \"black\", fill = rep(total_fill[custom_cols_p17],2)) + \n     ggtitle(\"Proportion of Kaggle Users per Platform by Gender\") +\n     geom_text(aes(label = Prop_str), position = position_stack(vjust = 0.45), size = 4) +\n     theme(axis.text.x = element_text(angle = 30, hjust = 1)) +\n     scale_x_discrete(limits = custom_cols_p17) +\n     theme(axis.title.x = element_blank(), axis.title.y = element_blank(), plot.title = element_text(size = 12)) +\n     facet_wrap(~Gender)\n\ngg17 <- ggplotly(p17)\nhtmlwidgets::saveWidget(gg17, \"gg17.html\")\ndisplay_html('<iframe src=\"gg17.html\" width=800 height=550 frameborder=\"0\"></iframe>')",
      "execution_count": null,
      "outputs": []
    },
