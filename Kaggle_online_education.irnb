      "source": "# Prepare dataframe of MOOC platforms by Country\nplot_frame <- multiple_choice_Q36 %>% mutate(Country = multiple_choice[2:nrow(multiple_choice),]$Q3) %>% \n        filter(Country != \"Other\") %>%\n        gather(key = \"Platform\", value = \"Num_Users\", -\"Country\") %>%\n        group_by(Country, Platform) %>% \n        summarise(Counts = sum(Num_Users), \n              Proportion = round(Counts/n(),2),\n              Prop_str = percent_to_string(Proportion))\n\n# Generate and display plot\np19 <- plot_frame %>% filter(Country %in% top_countries[[\"Country\"]][1:16]) %>%\n     ggplot(aes(x = Platform, y = Proportion)) + \n     geom_bar(stat = \"identity\", color = \"black\", fill = rep(total_fill[custom_cols_p18],16)) + \n     ggtitle(\"Proportion of Kaggle Users per Platform by Country\") +\n     geom_text(aes(label = Prop_str), position = position_stack(vjust = 0.45), size = 2) +\n     theme(axis.text.x = element_text(angle = 30, hjust = 1)) +\n     scale_x_discrete(limits = custom_cols_p18) +\n     theme(axis.title.x = element_blank(), axis.title.y = element_blank(), plot.title = element_text(size = 12)) +\n     facet_wrap(~Country)\n\ngg19 <- ggplotly(p19)\nhtmlwidgets::saveWidget(gg19, \"gg19.html\")\ndisplay_html('<iframe src=\"gg19.html\" width=800 height=800 frameborder=\"0\"></iframe>')",
