      "cell_type": "code",
      "source": "# Prepare dataframe for plotting\nplot_frame <- multiple_choice[2:nrow(multiple_choice), startsWith(colnames(multiple_choice), \"Q39\")][,1] %>% \n                mutate(Language = multiple_choice[2:nrow(multiple_choice),]$Q17) %>% \n                na.omit() \n\nplot_frame <- plot_frame %>% \n                filter(Language %in% top_languages[[\"Programming Language\"]][1:12]) %>% \n                group_by(Language, Q39_Part_1) %>% \n                summarise(Counts = n())\n\nlanguage_counts <- plot_frame %>% group_by(Language) %>% summarize(Total = sum(Counts))\n\nplot_frame[\"Proportion\"] <- round(plot_frame['Counts']/as.vector(sapply(language_counts$Total, rep, 6)), 2)\nplot_frame[\"Prop_str\"] <- percent_to_string(plot_frame[[\"Proportion\"]])\n\n# Generate and display plot\np29 <- plot_frame %>%    \n     ggplot(aes(x = Q39_Part_1, y = Proportion)) + \n     geom_bar(stat = \"identity\", color = \"black\", fill = rep(custom_fill[c(5,4,3,2,1,6)], 12)) + \n     ggtitle(\"Perception of MOOCs vs Brick and Mortar Institutions by Language\") +\n     geom_text(aes(label = Prop_str), position = position_stack(vjust = 0.45), size = 2) +\n     theme(axis.text.x = element_text(angle = 30, hjust = 1)) +\n     scale_x_discrete(limits = section3_cols) + xlab(\"MOOC Quality vs Traditional Education\") +\n     theme( axis.title.y = element_blank(), \n     plot.title = element_text(size = 12), axis.text.x = element_blank(), axis.ticks.x = element_blank()) +\n     facet_wrap(~Language)\n\ngg29 <- ggplotly(p29)\nhtmlwidgets::saveWidget(gg29, \"gg29.html\")\ndisplay_html('<iframe src=\"gg29.html\" width=800 height=800 frameborder=\"0\"></iframe>')",
