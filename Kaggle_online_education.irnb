      "cell_type": "code",
      "source": "# Create data frame grouped by langauge to plot\nby_language <- multiple_choice_Q35 %>% \n                mutate(Language = multiple_choice[2:nrow(multiple_choice),]$Q17) %>% \n                select(-Text) %>% na.omit() %>%\n                gather(key = \"Learning_Method\", value = \"Percentage\", -\"Language\") %>% \n                group_by(Language,Learning_Method) %>% \n                summarize(Counts = sum(Percentage > 0, na.rm=TRUE), \n                      Percent_Participation = round(Counts/sum(Percentage > -1, na.rm=TRUE),2),\n                      Percent_Participation_str = percent_to_string(Percent_Participation))\n\n# Generate and display plot\np10 <-  by_language %>% \n        filter(Language %in% top_languages[[\"Programming Language\"]][1:16]) %>%\n        ggplot(aes(x = Learning_Method, y = Percent_Participation)) + \n        geom_bar(stat = \"identity\", color = \"black\", fill = rep(custom_fill[c(1:5)], 16)) +\n        geom_text(aes(label = Percent_Participation_str), position = position_stack(vjust = 0.45), size = 3) +\n        ggtitle(\"Learning Source Participation by Programming Language\") +\n        scale_x_discrete(limits = names(multiple_choice_Q35)[1:5]) +\n        theme(axis.text.x = element_text(angle = 30, hjust = 1), axis.title.x = element_blank()) +\n        theme(axis.title.y = element_blank()) +\n        facet_wrap(~Language)\n\ngg10 <- ggplotly(p10)\nhtmlwidgets::saveWidget(gg10, \"gg10.html\")\ndisplay_html('<iframe src=\"gg10.html\" width=800 height=800 frameborder=\"0\"></iframe>')",
