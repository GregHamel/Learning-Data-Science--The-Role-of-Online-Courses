      "cell_type": "code",
      "source": "# Generate and display plot\np9 <-   multiple_choice_Q35 %>% mutate(Country = multiple_choice[2:nrow(multiple_choice),]$Q3) %>% \n        filter(Country != \"Other\" & Country != \"I do not wish to disclose my location\") %>% \n        select(-Text) %>%\n        gather(key = \"Learning_Method\", value = \"Percentage\", -\"Country\") %>%\n        group_by(Country,Learning_Method) %>% \n        summarize(Counts = sum(Percentage > 0, na.rm=TRUE), \n                  Percent_Participation = round(Counts/sum(Percentage > -1, na.rm=TRUE),2),\n                  Percent_Participation_str = percent_to_string(Percent_Participation)) %>%\n        filter(Country %in% top_countries[[\"Country\"]]) %>%\n        ggplot(aes(x = Learning_Method, y = Percent_Participation)) + \n        geom_bar(stat = \"identity\", color = \"black\", fill = rep(custom_fill[c(1:5)],20) ) +\n        geom_text(aes(label=Percent_Participation_str), position = position_stack(vjust = 0.45), size=2) +\n        ggtitle(\"Learning Source Participation by Country\") +\n        scale_x_discrete(limits = names(multiple_choice_Q35)[1:5]) +\n        theme(axis.text.x = element_text(angle = 30, hjust = 1), axis.title.x = element_blank()) +\n        theme(axis.title.y = element_blank()) +\n        facet_wrap(~Country)\n\ngg9 <- ggplotly(p9)\nhtmlwidgets::saveWidget(gg9, \"gg9.html\")\ndisplay_html('<iframe src=\"gg9.html\" width=800 height=800 frameborder=\"0\"></iframe>')",
