      "cell_type": "code",
      "source": "# Create dataframe of Coursera users for plotting\nplot_frame <- multiple_choice_Q36[multiple_choice_Q36[\"Coursera\"] == 1,] %>% \n                colSums() %>% \n                data.frame() \n\ncolnames(plot_frame) <- \"Num_Users\"\nplot_frame[\"Platform\"] <- row.names(plot_frame)\ntotal_users <- plot_frame[\"Coursera\", \"Num_Users\"]\nplot_frame <- plot_frame %>% filter(Platform != 'Coursera')\nplot_frame[\"Proportion\"] <- round(plot_frame[\"Num_Users\"]/total_users,2)\nplot_frame[\"Prop_str\"] <- percent_to_string(plot_frame[[\"Proportion\"]])\n\ncustom_cols_p15 <- plot_frame %>% \n                    arrange(desc(Num_Users)) \n\np15 <- plot_frame %>%    \n         ggplot(aes(x = Platform, y = Num_Users)) + \n         geom_bar(stat = \"identity\", color = \"black\", fill = total_fill[custom_cols_p15[[\"Platform\"]]]) + \n         ggtitle(\"Alternate Platform Use by Coursera Users\") +\n         geom_text(aes(label = Prop_str), position = position_stack(vjust = 0.45), size = 3) +\n         theme(axis.text.x = element_text(angle = 30, hjust = 1)) + ylab(\"Number of Users\") +\n         scale_x_discrete(limits = custom_cols_p15[[\"Platform\"]])\n\ngg15 <- ggplotly(p15)\nhtmlwidgets::saveWidget(gg15, \"gg15.html\")\ndisplay_html('<iframe src=\"gg15.html\" width=650 height=550 frameborder=\"0\"></iframe>')",
