      "cell_type": "code",
      "source": "# Prepare dataframe of platform use by Age\nplot_frame <- multiple_choice_Q36 %>% mutate(Age = multiple_choice[2:nrow(multiple_choice),]$Q2) %>% \n        gather(key = \"Platform\", value = \"Num_Users\", -\"Age\") %>%\n        group_by(Age, Platform)  %>% \n        summarise(Counts = sum(Num_Users), \n              Proportion = round(Counts/n(),2),\n              Prop_str = percent_to_string(Proportion))\n\ncustom_cols_p18 <- c('Coursera', 'Udemy', 'DataCamp', 'Udacity', 'edX', 'Kaggle.Learn')\n\n# Generate and display plot\np18 <- plot_frame %>%    \n     ggplot(aes(x = Platform, y = Proportion)) + \n     geom_bar(stat = \"identity\", color = \"black\", fill = rep(total_fill[custom_cols_p18],12)) + \n     ggtitle(\"Proportion of Kaggle Users per Platform by Age\") +\n     geom_text(aes(label = Prop_str), position = position_stack(vjust = 0.45), size = 2) +\n     theme(axis.text.x = element_text(angle = 30, hjust = 1)) +\n     scale_x_discrete(limits = custom_cols_p18) +\n     theme(axis.title.x = element_blank(), axis.title.y = element_blank(), plot.title = element_text(size = 12)) +\n     facet_wrap(~Age)\n\ngg18 <- ggplotly(p18)\nhtmlwidgets::saveWidget(gg18, \"gg18.html\")\ndisplay_html('<iframe src=\"gg18.html\" width=800 height=800 frameborder=\"0\"></iframe>')",
