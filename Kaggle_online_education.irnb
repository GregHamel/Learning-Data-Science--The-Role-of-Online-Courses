      "cell_type": "code",
      "source": "num_single_moocers <- sum(rowSums(multiple_choice_Q36) == 1)\n\n# Create dataframe for plotting\nplot_frame <- multiple_choice_Q36 %>% \n                filter(rowSums(multiple_choice_Q36) == 1) %>% \n                colSums() %>% \n                data.frame()\n\ncolnames(plot_frame) <- \"Num_Users\"\nplot_frame[\"Platform\"] <- row.names(plot_frame)\nplot_frame[\"Proportion\"] <- round(plot_frame[\"Num_Users\"]/num_single_moocers,2)\nplot_frame[\"Prop_str\"] <- percent_to_string(plot_frame[[\"Proportion\"]])\n\ncustom_cols_p14 <- plot_frame %>% arrange(desc(Num_Users)) \n\n# Generate and display plot\np14 <- plot_frame %>%\n     ggplot(aes(x = Platform, y = Num_Users)) + \n     geom_bar(stat = \"identity\", color = \"black\", fill = total_fill[custom_cols_p14[[\"Platform\"]]]) + \n     ggtitle(\"Single Platform Users\") +\n     geom_text(aes(label = Prop_str), position = position_stack(vjust = 0.45), size = 3) +\n     theme(axis.text.x = element_text(angle = 30, hjust = 1)) + ylab(\"Number of Users\") +\n     scale_x_discrete(limits = custom_cols_p14[[\"Platform\"]])\n\ngg14 <- ggplotly(p14)\nhtmlwidgets::saveWidget(gg14, \"gg14.html\")\ndisplay_html('<iframe src=\"gg14.html\" width=650 height=550 frameborder=\"0\"></iframe>')",
